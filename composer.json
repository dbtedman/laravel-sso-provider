{
  "name": "dbtedman/laravel-sso-provider",
  "description": "Provides custom SSO integration to Laravel 5 applications.",
  "license": "MIT",
  "version": "2.0.0",
  "scripts": {
    "test": "@php vendor/bin/phpunit",
    "test:cover": "@php vendor/bin/phpunit --coverage-text",
    "lint": "@php vendor/bin/phpcs -p",
    "format": "@php vendor/bin/phpcbf"
  },
  "require": {
    "php": "7.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.3.4",
    "squizlabs/php_codesniffer": "3.4.2"
  },
  "autoload": {
    "psr-4": {
      "DBTedman\\": "src/DBTedman/"
    }
  }
}
